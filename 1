#include <iostream>
#include <stdio.h>
using namespace std;
int main()
{
	int date;
	int month;
	cin >> date >> month;
	const char* k[12] = { "January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December" };
	const char* r[31] = { "first", "second", "third", "fourth", "fifth", "sixth", "seventh", "eighth", "ninth", "tenth", "eleventh", "twelfth", 
		"thirteenth", "fourteenth", "fifteenth", "sixteenth", "seventeenth", "eighteenth", "nineteenth", "twentieth", "twenty - first", "twenty - second", 
		"twenty - third", "twenty - fourth", "twenty - fifth", "twenty - sixth", "twenty - seventh", "twenty - eighth", "twenty - ninth", "thirtieth", "thirty - first"};
	printf("%s %s", k[month - 1], r[date - 1]);
}
